<createTable tableName="boxofcourse">
	<column name="boxid" type="character varying">
		<constraints  nullable="false" 	references="box(boxid)" deleteCascade="true" foreignKeyName="fk_boxofcourse_boxid" />
	</column>
	<column name="courseid" type="character varying">
		<constraints  nullable="false" 	references="course(courseid)" deleteCascade="true" foreignKeyName="fk_boxofcourse_courseid" />
	</column>
	<column name="autoadd" type="boolean">
		<constraints  nullable="false"  />
	</column>
</createTable>
