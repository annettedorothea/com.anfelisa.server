<createTable tableName="emailconfirmation">
	<column name="token" type="character varying">
		<constraints primaryKey="true" nullable="false"  />
	</column>
	<column name="userid" type="character varying">
		<constraints  nullable="false"  references="public.user(userid)" deleteCascade="true" foreignKeyName="fk_public.emailconfirmation_userid" />
	</column>
</createTable>
