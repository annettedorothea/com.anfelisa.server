<createTable tableName="loginlog">
	<column name="loginlogid" type="character varying">
		<constraints primaryKey="true" nullable="false"  />
	</column>
	<column name="username" type="character varying">
		<constraints  nullable="false" 	references="user(username)" deleteCascade="true" foreignKeyName="fk_loginlog_username" />
	</column>
	<column name="date" type="timestamp with time zone">
		<constraints  nullable="false"  />
	</column>
</createTable>
