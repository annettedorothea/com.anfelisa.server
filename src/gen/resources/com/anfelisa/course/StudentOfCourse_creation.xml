<createTable tableName="studentofcourse">
	<column name="username" type="character varying">
		<constraints  nullable="false" 	references="user(username)" deleteCascade="true" foreignKeyName="fk_studentofcourse_username" />
	</column>
	<column name="courseid" type="character varying">
		<constraints  nullable="false" 	references="course(courseid)" deleteCascade="true" foreignKeyName="fk_studentofcourse_courseid" />
	</column>
</createTable>
