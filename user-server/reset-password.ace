HttpServer 

com.anfelisa.user.resetpassword

scenarios 

	ResetPassword
		GIVEN
			com.anfelisa.user.forgotpassword.ForgotPasswordOK
		WHEN com.anfelisa.user.ResetPassword {
			token: "RESET-PW-TOKEN"
			password: "newPassword"
		}
		THEN 200

	ResetPasswordInvalidToken
		GIVEN
			com.anfelisa.user.forgotpassword.ForgotPasswordOK
		WHEN com.anfelisa.user.ResetPassword {
			token: "INVALID-TOKEN"
			password: "newPassword"
		}
		THEN 400

