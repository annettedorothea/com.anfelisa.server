			com.anfelisa.box.createbox.CreateBoxMinimal
		WHEN
			com.anfelisa.category.CreateCategory
				{
					categoryName: "lala"
				}
				authorization "Annette-${testId}" : "password"
		THEN 400

	CreateCategoryParentCategoryIsEmpty
